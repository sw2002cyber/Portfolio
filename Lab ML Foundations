        "train_survival = y_train.value_counts()\n",
        "print(f\"  Did Not Survive: {train_survival[0]} ({train_survival[0]/len(y_train)*100:.1f}%)\")\n",
        "print(f\"  Survived: {train_survival[1]} ({train_survival[1]/len(y_train)*100:.1f}%)\")\n",
        "\n",
        "print(f\"Test set survival distribution:\")\n",
        "test_survival = y_test.value_counts()\n",
        "print(f\"  Did Not Survive: {test_survival[0]} ({test_survival[0]/len(y_test)*100:.1f}%)\")\n",
        "print(f\"  Survived: {test_survival[1]} ({test_survival[1]/len(y_test)*100:.1f}%)\")"
      ]
