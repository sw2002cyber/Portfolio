      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "\n",
            "üìà STEP 3: FEATURE SELECTION WITH CORRELATION ANALYSIS...\n",
            "Features most correlated with survival (absolute value):\n",
            "Survived      1.000000\n",
            "Sex           0.694758\n",
            "Pclass        0.269344\n",
            "Fare          0.242294\n",
            "Embarked_S    0.105520\n",
            "Parch         0.100861\n",
            "Embarked_C    0.089480\n",
            "Age           0.049568\n",
            "Embarked_Q    0.042057\n",
            "SibSp         0.017527\n",
            "Name: Survived, dtype: float64\n",
            "\n",
            "üéØ SELECTED TOP 5 FEATURES:\n",
            "  1. Sex (correlation: 0.695, positive relationship)\n",
            "  2. Pclass (correlation: 0.269, negative relationship)\n",
            "  3. Fare (correlation: 0.242, positive relationship)\n",
            "  4. Embarked_S (correlation: 0.106, negative relationship)\n",
            "  5. Parch (correlation: 0.101, positive relationship)\n",
            "\n",
            "Selected training set shape: (1047, 5)\n",
            "Selected test set shape: (262, 5)\n"
          ]
        }
      ],
      "source": [
        "print(\"\\nüìà STEP 3: FEATURE SELECTION WITH CORRELATION ANALYSIS...\")\n",
        "\n",
        "# Create a temporary DataFrame for correlation analysis (using training data only)\n",
        "train_analysis_df = X_train.copy()\n",
        "train_analysis_df['Survived'] = y_train.values\n",
        "\n",
        "# Calculate correlation matrix\n",
        "correlation_matrix = train_analysis_df.corr()\n",
        "survival_correlations = correlation_matrix['Survived'].abs().sort_values(ascending=False)\n",
        "\n",
        "print(\"Features most correlated with survival (absolute value):\")\n",
        "print(survival_correlations)\n",
        "\n",
        "# Select top 5 features for modeling (excluding 'Survived' itself)\n",
        "top_features = survival_correlations.index[1:6].tolist()\n",
        "feature_names = top_features\n",
        "\n",
        "print(f\"\\nüéØ SELECTED TOP {len(top_features)} FEATURES:\")\n",
        "for i, feature in enumerate(top_features, 1):\n",
        "    correlation = survival_correlations[feature]\n",
        "    direction = \"positive\" if correlation_matrix.loc[feature, 'Survived'] > 0 else \"negative\"\n",
        "    print(f\"  {i}. {feature} (correlation: {correlation:.3f}, {direction} relationship)\")\n",
        "\n",
        "# Filter datasets to only include top features\n",
        "X_train_selected = X_train[top_features]\n",
        "X_test_selected = X_test[top_features]\n",
        "\n",
        "print(f\"\\nSelected training set shape: {X_train_selected.shape}\")\n",
        "print(f\"Selected test set shape: {X_test_selected.shape}\")"
      ]
    },
